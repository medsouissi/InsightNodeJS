              <div class="row">
                 
                    <% Produits.jData.forEach(function(produit) { %>
                        <div class="col-xxl-4 col-xl-4 col-lg-4 col-md-6">
                        <div class="product__item white-bg mb-30 wow fadeInUp" data-wow-delay=".3s">
                            <div class="product__thumb">
